[binaries]
c = '@CC@'
cpp = '@CXX@'
objc = '@CC@'
objcpp = '@CXX@'
ar = '@AR@'
ranlib = '@RANLIB@'
strip = 'true'

[properties]
root = '@SDK@'
needs_exe_wrapper = true
sys_root = '@SYSROOT@'
pkg_config_libdir = '@PKGCONFIG_LIBDIR@'

[host_machine]
system = 'darwin'
subsystem = '@SUBSYSTEM@'
kernel = 'xnu'
cpu_family = '@CPU_FAMILY@'
cpu = '@CPU@'
endian = 'little'
